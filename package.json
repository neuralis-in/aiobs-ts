      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@opentelemetry/instrumentation-openai": "^0.7.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "openai": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^1.0.0"
